<Page x:Class="InvoiceX.Pages.SettingsPage.SettingsMain"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:InvoiceX.Pages.SettingsPage"
      mc:Ignorable="d" 
      d:DesignHeight="950" d:DesignWidth="1725"
      Title="SettingsMain">

    <Grid Background="LightGray" Panel.ZIndex="1">
        <Grid Background="Transparent" HorizontalAlignment="Left" Height="70" Margin="0,-85,0,0" VerticalAlignment="Top" Width="646" Panel.ZIndex="1">
            <Button x:Name="btnUsers" Content="Users" Margin="0,10,0,0" Style="{StaticResource ButtonStyle}" Click="btnUsers_Click" />
            <Button x:Name="btnDatabase" Content="Database" Margin="130,10,0,0" Style="{StaticResource ButtonStyle}" Click="btnDatabase_Click" />
            
        </Grid>

        <Frame x:Name="settingsPage" NavigationUIVisibility="Hidden" Content="" HorizontalAlignment="Left" Height="950" VerticalAlignment="Top" Width="1725" RenderTransformOrigin="0.465,0.404"/>

    </Grid>
</Page>
